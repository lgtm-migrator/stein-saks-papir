{
  "extends": ["plugin:react/recommended", "airbnb"],
  "globals": {
    "cy": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": "warn",
    "no-use-before-define": "warn",
    "import/no-unresolved": "warn",
    "no-param-reassign": "warn",
    "import/extensions": [
      "warn",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
